{
	"name": "tailwindcss-color-suite",
	"version": "0.7.0",
	"author": "Jeff Schofield <jeff@jeffschofield.com>",
	"keywords": [
		"vite",
		"tailwind",
		"color",
		"palette",
		"generator",
		"theme"
	],
	"repository": "github:ShiftLimits/tailwindcss-color-suite",
	"bugs": {
		"url": "https://github.com/ShiftLimits/tailwindcss-color-suite/issues",
		"email": "jeff@jeffschofield.com"
	},
	"license": "MIT",
	"main": "dist/index.js",
	"module": "dist/index.mjs",
	"types": "dist/index.d.ts",
	"exports": {
		".": {
			"import": {
				"types": "./dist/index.d.mts",
				"import": "./dist/index.mjs"
			},
			"require": {
				"types": "./dist/index.d.ts",
				"require": "./dist/index.js"
			}
		},
		"./tailwindcss": {
			"import": {
				"types": "./dist/plugin.tailwindcss.d.mts",
				"import": "./dist/plugin.tailwindcss.mjs"
			},
			"require": {
				"types": "./dist/plugin.tailwindcss.d.ts",
				"require": "./dist/plugin.tailwindcss.js"
			}
		},
		"./svelte": {
			"import": {
				"types": "./dist/plugin.svelte.d.mts",
				"import": "./dist/plugin.svelte.mjs"
			},
			"require": {
				"types": "./dist/plugin.svelte.d.ts",
				"require": "./dist/plugin.svelte.js"
			}
		},
		"./app": {
			"import": "./dist/app.main/index.mjs",
			"require": "./dist/app.main/index.js"
		},
		"./client": {
			"types": "./client.d.ts"
		}
	},
	"files": [
		"bin/*",
		"dist/**/*",
		"CHANGELOG.md",
		"client.d.ts"
	],
	"scripts": {
		"build": "vite build && tsup",
		"dev": "vite",
		"test": "jest",
		"format": "npx -y eclint fix"
	},
	"bin": {
		"tailwindcss-color-suite": "./dist/cli.mjs"
	},
	"gitHooks": {
		"commit-msg": "node scripts/verifyCommit.js"
	},
	"devDependencies": {
		"@hyrious/esbuild-plugin-commonjs": "^0.2.4",
		"@jest/types": "^29.6.3",
		"@types/body-parser": "^1.19.5",
		"@types/jest": "^29.5.14",
		"@types/node": "^22.10.2",
		"@vitejs/plugin-vue": "^5.2.1",
		"@vue/compiler-sfc": "^3.5.13",
		"autoprefixer": "^10.4.20",
		"body-parser": "^1.20.3",
		"jest": "^29.7.0",
		"jest-matcher-css": "^1.1.0",
		"postcss": "^8.4.49",
		"tailwindcss": "^3.4.16",
		"ts-jest": "^29.2.5",
		"ts-node": "^10.9.2",
		"tsup": "^8.3.5",
		"typescript": "^5.7.2",
		"vite": "^6.0.3",
		"vite-plugin-svg-icons": "^2.0.1",
		"vue": "^3.5.13",
		"vue-router": "^4.5.0",
		"vuedraggable": "^4.1.0",
		"vuex": "^4.1.0",
		"yorkie": "^2.0.0"
	},
	"peerDependencies": {
		"esbuild": "^0.24.0",
		"tailwindcss": "^3.4.16",
		"vite": "^6.0.3"
	},
	"dependencies": {
		"bundle-require": "^5.0.0",
		"chalk": "^5.3.0",
		"fit-curve": "^0.2.0",
		"body-parser": "^1.20.3"
	}
}
